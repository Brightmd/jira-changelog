{
  "name": "jira-changelog",
  "version": "1.5.2",
  "description": "Generates a changelog by matching git commits to Jira tickets.",
  "repository": "https://github.com/jgillick/jira-changelog",
  "main": "dist/index.js",
  "bin": {
    "jira-changelog": "dist/scripts/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "babel src --out-dir dist/scripts -s; cp changelog.config.js ./dist/",
    "clean": "rm -r dist || true",
    "test": "jest",
    "prepare": "npm run build"
  },
  "author": "Jeremy Gillick",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "jest": "^25.1.0"
  },
  "dependencies": {
    "commander": "^2.20.0",
    "core-js": "^3.6.4",
    "ejs": "^2.6.1",
    "form-urlencoded": "^2.0.9",
    "haikunator": "^2.1.1",
    "html-entities": "^1.2.1",
    "jira-client": "^6.8.0",
    "lodash": "^4.17.15",
    "node-fetch": "^1.7.3",
    "promise-throttle": "^1.0.1",
    "regenerator-runtime": "^0.13.3",
    "request": "^2.88.0",
    "simple-git": "^1.131.0",
    "source-map-support": "^0.5.16"
  }
